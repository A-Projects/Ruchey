// Russian engineering Latin keyboard layout (Ручей)
// Based on the English standard keyboard layout
// Baryshkin Andrey <baryshkin_aa@codeblast.ru>
// Last changes 2022/07/02
// Version 1.3
partial alphanumeric_keys
xkb_symbols "ruchey_latin" {

    name[Group1]= "Russian (Engineering, Latin)";

    include "us(basic)"

    key <TLDE> { [ VoidSymbol,   VoidSymbol,          U00A7,      U0301 ] };
    key <AE01> { [          1,       exclam,          U20BD, VoidSymbol ] };
    key <AE02> { [          2,   apostrophe,             at, VoidSymbol ] };
    key <AE03> { [          3,   numerosign,     numbersign, VoidSymbol ] };
    key <AE04> { [          4,    semicolon,         dollar, VoidSymbol ] };
    key <AE05> { [          5,     quotedbl,        percent, VoidSymbol ] };
    key <AE06> { [          6,        colon,    asciicircum, VoidSymbol ] };
    key <AE07> { [          7,     question,      ampersand, VoidSymbol ] };
    key <AE08> { [          8,     asterisk,     asciitilde, VoidSymbol ] };
    key <AE09> { [          9,    parenleft,  guillemotleft, VoidSymbol ] };
    key <AE10> { [          0,   parenright, guillemotright, VoidSymbol ] };
    key <AE11> { [      minus,   underscore,     VoidSymbol, VoidSymbol ] };
    key <AE12> { [      equal,         plus,     VoidSymbol, VoidSymbol ] };

    key <AD11> { [ VoidSymbol,   VoidSymbol,      braceleft, VoidSymbol ] };
    key <AD12> { [ VoidSymbol,   VoidSymbol,     braceright, VoidSymbol ] };

    key <AC10> { [ VoidSymbol,   VoidSymbol,    bracketleft, VoidSymbol ] };
    key <AC11> { [ VoidSymbol,   VoidSymbol,   bracketright, VoidSymbol ] };

    key <AB08> { [ VoidSymbol,   VoidSymbol,           less, VoidSymbol ] };
    key <AB09> { [ VoidSymbol,   VoidSymbol,        greater, VoidSymbol ] };
    key <AB10> { [     period,        comma,          grave, VoidSymbol ] };

    key <BKSL> { [      slash,    backslash,            bar, VoidSymbol ] };
    key <LSGT> { [      slash,    backslash,            bar, VoidSymbol ] };

    include "kpdl(dot)"
};

